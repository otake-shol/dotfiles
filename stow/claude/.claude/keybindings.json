{
  "bindings": [
    {
      "key": "ctrl+l",
      "command": "clear",
      "description": "画面クリア"
    },
    {
      "key": "ctrl+o",
      "command": "open_in_editor",
      "description": "エディタで開く"
    },
    {
      "key": "ctrl+r",
      "command": "retry",
